<div class="row">
  <div class="col-md-8">
    <h1><%= @wiki.title %></h1>
    <%= link_to "Edit", edit_wiki_path, class: "btn btn-sm btn-success" %>
    <%= link_to "Delete", @wiki, method: :delete, class: "btn btn-sm btn-danger", data: {confirm: 'Are you shure you want to delete this wiki?'} %>
    <p>
      <%= @wiki.body %>
    </p>
  </div>
</div>
