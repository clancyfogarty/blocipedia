<h3>List of Wikis</h3>

<div class="col-md-8">
  <% @wikis.each do |wiki| %>
    <div class="media clearfix">
      <div class="media-body ">
        <h4 class="media-heading">
          <%= link_to wiki.title, wiki %>
        </h4>
      </div>
    </div>
    <% end %>
  </div>
  <div class="col-md-4">
    <% if user_signed_in? %>
      <%= link_to "New Wiki", new_wiki_path, class: "btn btn-primary" %>
    <% end %>
  </div>
